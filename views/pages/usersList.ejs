<table id="myTable" class="table table-striped table-condensed custom-table display">
  <thead>
    <tr class="sortable">
      <%if (isCheckList) {%>
        <th></th>
      <% } %>
      <th>ID</th>
      <th>Username</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i=0; i<users.length; i++) {%>
      <tr>
        <%if (isCheckList) {%>
          <td><input class="checkboxes" type="checkbox" data-user_id="<%= users[i].id %>"></td>
        <% } %>
        <td><%= users[i].id %></td>
        <td><%= users[i].user_name %></td>
        <td><%= users[i].first_name %></td>
        <td><%= users[i].last_name %></td>
        <td><%= users[i].email %></td>
      </tr>
      <% } %>
    </tbody>
    <tfoot>
      <tr>
        <td colspan="11" class="text-center">
          <div st-pagination="" st-items-by-page="pageSize" st-displayed-pages="5"></div>
        </td>
      </tr>
    </tfoot>
  </table>
